<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefWidth="149.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_project.controllers.EditProducerController">
    <Label prefHeight="29.0" prefWidth="78.0" text="Edit Producer" />
    <TextField fx:id="nameField" promptText="Name" />
    <TextField fx:id="cinField" promptText="CIN" />
    <TextField fx:id="addressField" promptText="Address" />
    <TextField fx:id="phoneNumberField" promptText="Phone Number" />
    <Button onAction="#saveChanges" text="Update" />
</VBox>
