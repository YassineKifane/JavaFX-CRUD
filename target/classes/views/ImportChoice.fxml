<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="228.0" prefWidth="244.0" spacing="10" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafx_project.controllers.ImportController">
    <Label style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Import Choice" />

    <Button mnemonicParsing="false" fx:id="importOutput" onAction="#importTextFile" prefHeight="25.0" prefWidth="115.0" text="Text File" />
    <Button mnemonicParsing="false" fx:id="importJson" onAction="#importJsonFile" prefHeight="25.0" prefWidth="115.0" text="JSON File" />
    <Button mnemonicParsing="false" fx:id="importExcel" onAction="#importExcelFile" prefHeight="25.0" prefWidth="115.0" text="Excel File" />

    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>
</VBox>
